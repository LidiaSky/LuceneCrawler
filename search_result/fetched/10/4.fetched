function tmp_removeCite() {
    if ($('#cited_by_articles').length) {
    if (SDM.pm != undefined) {
        if (SDM.pm.issnisbn == '23528478') {
            $('#cited_by_articles').hide();
            } 
        } 
    }
}

function tmp_removeCiteCounter(c) {
    if ($('#cited_by_articles').length) {
        tmp_removeCite();
    }
    else if (c>30) {
        return;
    }
    else {
        c++;
        setTimeout(function() {tmp_removeCiteCounter(c);}, 200);
    }
}
tmp_removeCiteCounter(1);  
</script>


