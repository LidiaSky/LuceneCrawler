<SCRIPT LANGUAGE="JavaScript" type="text/javascript">

<!-- Hide Script from non-JavaScript Browsers

function checkFavoriteJournals (formSelect, conType, isLoggedIn, hasFavJrnl)
{

  var indexToKeepSelected = 0;
  var valueToCheck = "2";
  var selectedValue = formSelect.value; 
  var alertMsg = "You do not have a favorite ";
  alertMsg += conType;
  alertMsg += " list. ";
  alertMsg += "To add a journal or book as a favorite, ";
  alertMsg += "please go to the journal/book page and click on the 'Add to Favorites' link.";

  if ((formSelect.checked  == true || selectedValue == valueToCheck) && (!isLoggedIn || !hasFavJrnl))
  {
    formSelect.checked =false;
    formSelect.selectedIndex = indexToKeepSelected;
    alert (alertMsg);
  }
};
function checkFavoriteJournalBooks (favSelect, isLoggedIn, hasFavJrnl, hasFavBook){
  var indexToKeepSelected = 0;
  var valueToCheck = "2";
  var formSelect = $('#'+ favSelect);
  var selectedValue = formSelect.val();
  var conType = "sources";;
  
  if ((formSelect.prop('checked')  == true && selectedValue == valueToCheck) && (!isLoggedIn || !hasFavJrnl || !hasFavBook))
  {
    if($('.limitByRow #journals').prop('checked') && hasFavJrnl){
        return;
    }
    else if($('.limitByRow #journals').prop('checked') && !hasFavJrnl){
        conType = "journal";
    }
    if($('.limitByRow #books').prop('checked') && hasFavBook){
        return;
    }
    else if($('.limitByRow #books').prop('checked') && !hasFavBook){
        conType = "book";
    }
    if( !hasFavJrnl && !hasFavBook ){
        conType = "sources";
    }
    var alertMsg = "You do not have a favorite ";
    alertMsg += conType;
    alertMsg += " list. ";
    alertMsg += "To add a journal or book as a favorite, ";
    alertMsg += "please go to the journal/book page and click on the 'Add to Favorites' link.";
    formSelect.prop('checked',false);
    $("input[name='Subscribed']").prop('checked',false);
    $("input[name='openaccess']").prop('checked',false);
    $("input[name='Subscribed']").eq(indexToKeepSelected).prop('checked',true);
    alert (alertMsg);
  }
}
//-->

