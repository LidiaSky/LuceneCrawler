      function targetVisible($element, visible) {
        var pageTop = $(window).scrollTop(),
          pageBottom = pageTop + $(window).height(),
          elementTop = $element.offset().top,
          elementBottom = elementTop + $element.height();   
          
         if (visible === true) {
            return ((pageTop < elementTop) && (pageBottom > elementBottom));
         } 
         else {
            return ((elementTop <= pageBottom) && (elementBottom >= pageTop));
         }  
          
      }
      var count = 0;
      var $element = $("#bar_chart");
      $( window ).on( "scroll", function( event ) {

          if (targetVisible( $element, false) === true && count == 0) {
                  $('.bgd-orange').addClass("trans-orange");
                  $('.bgd-grey').addClass("trans-grey");
                  $('.bgd-blue').addClass("trans-blue");
                  count++;
          }
          else {
              $( this ).off( "scroll", event );
          }
      }); 
